[ignore]

[include]

[libs]

[options]
module.name_mapper.extension='css' -> 'CSSModule'
module.name_mapper.extension='scss' -> 'CSSModule'
module.name_mapper.extension='postcss' -> 'CSSModule'
module.name_mapper.extension='sass' -> 'CSSModule'
module.name_mapper.extension='less' -> 'CSSModule'
module.name_mapper.extension='styl' -> 'CSSModule'
module.name_mapper.extension='stylus' -> 'CSSModule'

module.name_mapper.extension='png' -> 'WebpackAsset'
module.name_mapper.extension='jpg' -> 'WebpackAsset'
module.name_mapper.extension='jpeg' -> 'WebpackAsset'
module.name_mapper.extension='gif' -> 'WebpackAsset'
module.name_mapper.extension='svg' -> 'WebpackAsset'

module.name_mapper.extension='woff' -> 'WebpackAsset'
module.name_mapper.extension='woff2' -> 'WebpackAsset'
module.name_mapper.extension='eot' -> 'WebpackAsset'
module.name_mapper.extension='ttf' -> 'WebpackAsset'
module.name_mapper.extension='otf' -> 'WebpackAsset'

module.name_mapper.extension='html' -> 'WebpackAsset'

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue

module.system=haste
strip_root=true
