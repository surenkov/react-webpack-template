{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "safari >= 9",
          "ie >= 11"
        ],
        "node": "current"
      },
      "useBuiltIns": true,
      "loose": true,
      "modules": false
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-flow-strip-types",
    "transform-react-jsx",
    "react-hot-loader/babel"
  ],
  "comments": false
}
